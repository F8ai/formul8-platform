modules = ["nodejs-20", "web", "postgresql-16", "python-3.11", "python3"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-24_05"
packages = ["bash", "bc", "cairo", "catch2", "comic-neue", "coordgenlibs", "eigen", "ffmpeg-full", "freetype", "gh", "ghostscript", "git-lfs", "glibcLocales", "gobject-introspection", "gtk3", "iana-etc", "inchi", "libev", "libxcrypt", "libyaml", "maeparser", "pkg-config", "qhull", "rapidjson", "tcl", "tk", "wget", "xsimd"]

[deployment]
deploymentTarget = "autoscale"
build = ["sh", "-c", "node build.js"]
run = ["sh", "-c", "node start.js"]

[env]
PORT = "5000"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[agent]
integrations = ["javascript_anthropic==1.0.0", "javascript_gemini==1.0.0", "javascript_log_in_with_replit==1.0.0", "javascript_database==1.0.0", "javascript_xai==1.0.0", "javascript_openai==1.0.0", "javascript_object_storage==1.0.0"]
